<?xml version="1.0"?>
<launch>
    <env name="ROSCONSOLE_CONFIG_FILE" value="$(find localization)/config/rosconsole.config"/>

    <arg name="landmarks" default="$(find localization)/config/landmarks.yaml" />
    <arg name="initialPose" default="$(find localization)/config/initialPose.yaml" />

    <rosparam file="$(arg landmarks)" command="load" />
    <rosparam file="$(arg initialPose)" command="load" />

    <node pkg="localization" type="main" name="main" output="screen" />
</launch>